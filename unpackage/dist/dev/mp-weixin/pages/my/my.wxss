
.my_page{
	background-image: url(data:image/png;base64,);
	background-size: 100% auto;
	background-repeat: no-repeat;
	background-position-y: -130rpx;
	padding-top: 212rpx;
}
.my_info{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	font-size: 24rpx;
	color: #000000;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
}
.my_photo{
	width: 131rpx;
	height: 131rpx;
	border-radius: 50%;
	margin-bottom: 10rpx;
}
.my_lis{
	width: 678rpx;
	background-color: #ffffff;
	-webkit-box-shadow: 0px 0px 9rpx 1rpx 
		rgba(0, 0, 0, 0.05);
	        box-shadow: 0px 0px 9rpx 1rpx 
		rgba(0, 0, 0, 0.05);
	border-radius: 10rpx;
	margin: 0 auto;
	margin-top: 40rpx;
	padding: 0 30rpx;
}
.my_li_item{
	height: 90rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	border-bottom: solid 1px #ececec;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	padding: 0 25rpx;
}
.my_li_item:last-child{
	border: none;
}
.li_item_left{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	font-size: 24rpx;
	color: #000000;
}
.li_item_icon{
	width: 35rpx;
	height: 35rpx;
	margin-right: 30rpx;
}
.right_icon{
	width: 12rpx;
	height: 22rpx;
}
.jszc{
	position: fixed;
	left: 50%;
	-webkit-transform: translateX(-50%);
	    -ms-transform: translateX(-50%);
	        transform: translateX(-50%);
	bottom: 50rpx;
	font-size: 20rpx;
	color: #999999;
}

