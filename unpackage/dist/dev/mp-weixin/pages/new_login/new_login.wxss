
.new_logins{
	padding: 30rpx 36rpx;
}
.source_main{
	width: 678rpx;
	height: 550rpx;
	background-color: #ffffff;
	border-radius: 10rpx;
	padding: 0 40rpx;
}
.source_title{
	height: 80rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	font-size: 24rpx;
	color: #000000;
	border-bottom: solid 1px #ededed;
}
.source_item{
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	font-size: 20rpx;
}
.source_item.active{
	color: #00b0fc;
}
.source_item.active .source_checkbox{
	background: #00b0fc;
}
.source_checkbox{
	width: 20rpx;
	height: 20rpx;
	border: solid 1px #00b0fc;
}
.source_message{
	font-size: 20rpx;
	color: #000000;
	margin-top: 40rpx;
}
.source_submit{
	margin: 0 auto;
	width: 120rpx;
	height: 50rpx;
	background-color: #00b0fc;
	border-radius: 25rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	font-size: 20rpx;
	color: #ffffff;
	margin-top: 40rpx;
}

